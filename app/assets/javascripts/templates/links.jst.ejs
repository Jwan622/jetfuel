<% sorted.forEach(function(link) { %>
  <li>
    <strong> Title: </strong> <span class="lead"><%= link.title %></span><br>
    <em>created_at:</em> <%= link.created_at %> <br>
    <strong>Original Url:</strong>
    <%= _.trunc(link.orig_url, {
      'length': 130,
      'omission': "...(Hi Steve!!! This link is too long...so I truncated it with this extra long description.)"
    }) %>
    <br>
    <strong>Shortened Url:</strong> <a class="short-link" href="/links/<%= link.id %>">bit.ly-remix/_cnE7qOK</a>
    <strong>Visits:</strong> <%= link.clicks %>
  </li>
  </br>
<% }); %>
